--- a/daemons/fenced/Makefile.am
+++ b/daemons/fenced/Makefile.am
@@ -13,7 +13,7 @@
 
 halibdir	= $(CRM_DAEMON_DIR)
 
-halib_PROGRAMS	= pacemaker-fenced cts-fence-helper
+halib_PROGRAMS	= pacemaker-fenced
 
